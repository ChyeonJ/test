package exam;

import java.util.Scanner;

public class Q8393 {

	public static void main(String[] args) {
		// n이 주어졌을 때, 1부터 n까지 합을 구하는 프로그램을 작성하시오.
		
		Scanner keyboard = new Scanner(System.in);
		int n = keyboard.nextInt();
		
		int i = 1;
		int hap = 0;
		
		while (i < n+1) {
			
			hap = 1 + i;
			System.out.println(hap);
			i++;
			System.out.println(i);
			
		}
		System.out.println(hap);
	}

}
