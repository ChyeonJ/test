package exam;

import java.util.Scanner;

public class HomeWork_260119 {

	public static void main(String[] args) {
		//2부터 100_000까지의 숫자 중 소수 (Prime Number : 약수가 1과 자신만 있는 수)만 출력하는 프로그램 작성하시오.
		//소수 1과 자기 자신만 나눠 떨어지는 수 2 3 5 7 11 13 17 
		for(int i = 2; i <= 100_000; i++) {	//for문을 통해 100_000번 반복
			if(i % 4 == 0) {	// % 사용하여 2의 배수를 제거 -> 2는 소수이니 그의 제곱은 4를 나눔
			}
			else if(i % 6 == 0){// % 사용하여 6의 배수를 제거 -> 6는 소수이니 그의 제곱은 6을 나눔
			}
			else if(i % 10 == 0) {// % 사용하여 10의 배수를 제거 -> 5는 소수이니 그의 제곱은 10을 나눔
			}
			else if( i % 14 == 0){// % 사용하여 14의 배수를 제거 -> 14는 소수이니 그의 제곱은 14를 나눔
			}else if (i % 22 == 0) {// % 사용하여 22의 배수를 제거 -> 22는 소수이니 그의 제곱은 22를 나눔
			}
			else {
				System.out.println(i);
			}
			
		}

	}

}
