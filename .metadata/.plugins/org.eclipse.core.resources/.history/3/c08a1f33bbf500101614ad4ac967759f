package java_exam;

public class MethodExam {
	
	public static void printMyName(String name) {
		System.out.println("제 이름은" + name +"입니다");
	}
	
	// 프로그램이 실행될 수 있는 메소드를 생성한다.
	public static void main(String [] args) {	//JVM이 실행하고 -> 메인 Thread가 실행된다 위 아래
		
		printMyName();
		printMyName();
		printMyName();
	}
	
	

}
