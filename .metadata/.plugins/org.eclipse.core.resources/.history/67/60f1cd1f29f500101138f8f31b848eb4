package exam;

import java.util.Scanner;

public class HomeWork_260119 {

	public static void main(String[] args) {
		//2부터 100_000까지의 숫자 중 소수 (Prime Number : 약수가 1과 자신만 있는 수)만 출력하는 프로그램 작성하시오.
		//소수 1과 자기 자신만 나눠 떨어지는 수 2 3 5 7 11 13 17 
		
		for(int j = 2; j <= 11; j++) {
			System.out.println(j);
		}
		
		for(int i = 2; i <= 100_000; i++) {	//for문을 통해 100_000번 반복
			if(i % 2 == 0) {	// %를 사용하여 1과 자기 자신만 있는 숫자를 노출
			}
			else if(i % 3 == 0){
			}
			else if(i % 5 == 0) {
			}
			else if( i % 7 == 0){
			}else if (i % 11 == 0) {
			}
			else {
				System.out.println(i);
			}
			
		}

	}

}
