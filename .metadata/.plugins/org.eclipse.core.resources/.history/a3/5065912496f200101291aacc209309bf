package java_exam;

public class CompareExam {

	public static void main(String[] args) {
		
		// 10대가 맞다면 true를 아니라면 false를 출력한다.
		
		int age = 10;
		
		// age가 10대인지 확인해본다.
		// 10 <= age < 20
		boolean ageTrue = 10 <= age && age < 20;
		System.out.println(ageTrue);
		
		// age가 20대인지 확인해본다.
		// 20 <= age < 30
		boolean ageFalse = age < 30 && 20 <= age;
		System.out.println(ageFalse);
		
	}

}
