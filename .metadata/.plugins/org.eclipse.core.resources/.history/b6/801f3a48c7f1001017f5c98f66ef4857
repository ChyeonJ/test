package java_exam;

//class : 프로그램의 최소 실행 단위. 단위의 이름은 HelloWorld
// HelloWorld의 이름은 파일의 이름과 클래스의 이름은 항상 같아야한다.
public class HelloWorld {
	
	//메소드 : 프로그램이 어떤 방법으로 실행이 될 것인지 작성하는 영역
	// public static void main(String[] args) {..}
	//HelloWorld 클래스를 실행시킬 수 있는 메소드.
	public static void main(String[] args) {
		
		// "Hello World!" 문장을 출력해보자!
		// 출력을 위한 코드 : System.out.println(출력 문장 혹은 수식);
		System.out.println("Hello, World");
		System.out.println(30303030);
		System.out.println(93.45);
		System.out.println(3 + 3);
		System.out.println(3 - 2);
		System.out.println(3 / 2);
		System.out.println(3 * 3);
		
		// 실행 방법 	Window: Ctrl + Shift + F11
		// 			MAC : Command + Shift + F11
	}

}
