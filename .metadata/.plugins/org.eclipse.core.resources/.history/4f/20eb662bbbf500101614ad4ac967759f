package java_exam;

public class MethodExam {
	
	public static void printMyName() {
		System.out.println("제 이름은 최현종 입니다");
	}
	
	// 프로그램이 실행될 수 있는 메소드를 생성한다.
	public static void main(String [] args) {	//JVM이 실행하고 -> 메인 Thread가 실행된다 위 아래
		
		System.out.println("제 이름은 최현종 입니다.");
		System.out.println("제 이름은 땡땡땡 입니다.");
		System.out.println("제 이름은 000 입니다.");
		printMyName();
		printMyName();
		printMyName();
	}
	
	

}
