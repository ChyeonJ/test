package oop;

public class StudentTest {
	
	public static void main(String [] args) {
		Student student = new Student();
		
		student.java = 100;
		student.python = 100;
		student.cpp = 100;
		student.csharp = 100;
		
		//getSumAllScores
		int resultSum = student.getSunAllScores();
		System.out.println(resultSum);
		
		//getAvarage
		double resultAverage = student.getAverage();
		System.out.println(resultAverage);
		
		//getCourseCredit
		double resultGrade = student.getCourseCredit();
		System.out.println(resultGrade);
		
		//getABCDE
		String resultTotalGrade = student.getABCDF();
		System.out.println(resultTotalGrade);
		
	}
	
}
