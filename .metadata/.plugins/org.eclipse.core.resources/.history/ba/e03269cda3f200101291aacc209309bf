package java_exam;

public class ifExam {

	public static void main(String[] args) {
		
		// 0.123132121321321 ==> 12.313212132121 ==> 12
		int randomNumber = (int) (Math.random() * 100);
		System.out.println(randomNumber);
		
		//randomNumber == 16 : "10범위 숫자가 나왔습니다"
		//randomNumber == 36 : "30범위 숫자가 나왔습니다"
		//randomNumber == 66 : "60범위 숫자가 나왔습니다"
		//randomNumber == 6 : "0범위 숫자가 나왔습니다"
		//randomNumber == 27 : "20범위 숫자가 나왔습니다"
		//randomNumber == 55 : "50범위 숫자가 나왔습니다"
		
		int radix = randomNumber / 10 * 10;
		System.out.println(radix + "범위의 숫자가 나왔습니다.");
	}

}
